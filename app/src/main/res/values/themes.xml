<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.InverseMarket" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FF8D28</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->
    </style>
    <style name="nav" parent="Animation.Design.BottomSheetDialog">
        <item name="android:background">#FFFFFF</item>
        <item name="itemIconTint" tools:targetApi="o">#FF8D28</item>
        <item name="itemTextColor" tools:targetApi="o">#FF8D28</item>


    </style>
    <style name="nav2" parent="Animation.Design.BottomSheetDialog">
        <item name="android:background">#FFFFFF</item>
        <item name="itemIconTint" tools:targetApi="o">#000000</item>
        <item name="itemTextColor" tools:targetApi="o">#121212</item>


    </style>
</resources>